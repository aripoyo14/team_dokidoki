   "source": [
    "import sqlite3\n",
    "\n",
    "# SQLiteデータベースを作成\n",
    "conn = sqlite3.connect('dokidoki_diary.db')\n",
    "c = conn.cursor()\n",
    "\n",
    "# テーブルを作成\n",
    "c.execute('''\n",
    "            DELETE FROM DIary_table\n",
    "            WHERE diary_id >=28\n",
    "          ''')\n",
    "\n",
    "conn.commit()\n",
    "conn.close()"
   ]
  }
